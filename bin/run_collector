#!/usr/bin/env bash
set -euxo pipefail
export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"
eval "$(pyenv init -)"
eval "$(pyenv virtualenv-init -)"
pyenv activate collector-env
CO_DIR="$HOME/trading-bots/collector"
SCRIPT="$CO_DIR/collector/normalize_trades.py"
cd "$CO_DIR"
exec python "$SCRIPT" "$@"
